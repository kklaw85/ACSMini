                    <Condition Binding="{Binding Path=(Validation.HasError),ElementName=Num1_textbox}" Value="True"/>
                    <Condition Binding="{Binding Path=(Validation.Errors)[0].ErrorContent, ElementName=Num1_textbox}" Value="InputInt32"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="{StaticResource BR_TEXT_INVALID}"/>
            </MultiDataTrigger>
            <MultiDataTrigger>
