<Pagebase:PageBase
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" 
			 xmlns:MahApp="http://metro.mahapps.com/winfx/xaml/controls"
             xmlns:Panels="clr-namespace:NeoWisePlatform.SystemControls.Panels"
			 xmlns:Panels2="clr-namespace:NeoWisePlatform.CommonControls"
			 xmlns:Pagebase="clr-namespace:HiPA.Common.UControl;assembly=HiPA.Common"
             x:Class="NeoWisePlatform.UI.SystemControls.SubPages.Ctrl_ManufacturerBypassOption"
             mc:Ignorable="d" 
             Width="1625" Height="1004">
    <Grid x:Name="Grid">
        <StackPanel Orientation="Vertical">
            <Label Content="{StaticResource ManufacturerOverride}" Style="{StaticResource LB_MainTitle}"/>
            <StackPanel Margin="10" x:Name="StkPnlCtrlLst">
                <CheckBox IsChecked="{Binding Vision.BypassVisionCapture}" Content="Vision Capture" Margin="0,0,0,5"/>
                <CheckBox IsChecked="{Binding Vision.BypassVisionProcessing}" Content="Vision Processing" Margin="0,0,0,5"/>
            </StackPanel>
		</StackPanel>

	</Grid>
</Pagebase:PageBase>
