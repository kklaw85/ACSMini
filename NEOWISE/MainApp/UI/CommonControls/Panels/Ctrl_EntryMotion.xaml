<Panelbase:PanelBase
			 xmlns:Panelbase="clr-namespace:HiPA.Common.UControl;assembly=HiPA.Common"
			 x:Class="NeoWisePlatform.UI.CommonControls.Panels.Ctrl_EntryMotion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:mahapp="http://metro.mahapps.com/winfx/xaml/controls"
			 mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Label x:Name="Label" Style="{StaticResource STYL_GENERAL_LBL_Right}" Margin="0,0,5,0"/>
        <mahapp:NumericUpDown x:Name="Entry" StringFormat="F3" Margin="0,0,5,0" />
        <Button x:Name="Apply" Content="{StaticResource Apply}" Width="50" Margin="0,0,5,0" Click="Apply_Click"/>
        <Button x:Name="Goto" Content="{StaticResource Jog_AbsMove}" Width="50" Click="Goto_Click"/>
    </StackPanel>
</Panelbase:PanelBase>
